{
  "id": "global-governance",
  "category": "system",
  "description": "Global governance rules applied to all WyneOS components.",
  "rules": [
    "no-unsigned-execution",
    "no-unregistered-service-start",
    "require-manifest-integrity",
    "require-telemetry-stream",
    "enforce-safe-io"
  ],
  "severity": "critical",
  "allowedActions": [],
  "deniedActions": [
    "unsafe-write",
    "unauthorised-network-call",
    "tamper-log",
    "external-exec"
  ],
  "complianceLevel": "strict"
}
